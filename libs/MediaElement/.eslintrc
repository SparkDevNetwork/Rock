{
  "env": {
    "es6": true,
    "browser": true,
    "mocha": true,
    "node": true,
    "jquery": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "no-undef" : 0,
    "no-case-declarations": 0,
    "no-empty": 0
  }
}