                            <hr />

                            <h4 class="heading-individual">Second Adult</h4>

                            <div class="adult-2-fields">
                                <div class="row">
                                    <div class="col-sm-6">
                                        <StaticFormControl
                                                           v-if="adult2.isFirstNameReadOnly"
                                                           label="First Name"
                                                           :modelValue="adult2.firstName" />
                                        <TextBox v-else
                                                 v-model="adult2.firstName"
