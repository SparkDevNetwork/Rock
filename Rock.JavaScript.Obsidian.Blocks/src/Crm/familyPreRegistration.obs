                                                             :disabled="isSaving || isNavigating"
                                                             label="Username"
                                                             :rules="config.createAccountField.isRequired ? 'required' : ''"
                                                             @blur="checkUsernameAvailability" />
                                                    <dl>
                                                        <dt></dt>
