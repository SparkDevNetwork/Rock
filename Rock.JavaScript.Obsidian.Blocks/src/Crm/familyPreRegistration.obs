                            <div class="row mt-3">
                                <div class="col-md-12">
                                    <div v-if="config.createAccountField?.isShown">
                                        <div class="well card-createaccount">
                                            <h4 class="heading-createaccount">{{ config.createAccountTitle }}</h4>
                                            <p>{{ config.createAccountDescription }}</p>
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <TextBox
