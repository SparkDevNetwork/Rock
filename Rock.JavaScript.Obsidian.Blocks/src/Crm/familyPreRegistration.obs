                                                        :rules="config.adultMaritalStatusField.isRequired ? adult1RequiredRules : ''"
                                                        :showBlankItem="true" />
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-6">
                                    <PhoneNumberBox
                                                    v-if="config.adultMobilePhoneField?.isShown"
