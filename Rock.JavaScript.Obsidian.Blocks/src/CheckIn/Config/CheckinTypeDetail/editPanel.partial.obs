        updateRefValue(autoSelectOptions, props.modelValue?.bag?.generalSettings?.autoSelectOptions ?? "");
        updateRefValue(useSameOptions, props.modelValue?.bag?.generalSettings?.useSameOptions ?? false);
        updateRefValue(preventInactivePeople, props.modelValue?.bag?.generalSettings?.preventInactivePeople ?? false);
        updateRefValue(preventDuplicateCheckin, props.modelValue?.bag?.generalSettings?.preventDuplicateCheckin ?? false);
        updateRefValue(promotionsContentChannelTypes, props.modelValue?.bag?.generalSettings?.promotionsContentChannelTypes ?? "");

