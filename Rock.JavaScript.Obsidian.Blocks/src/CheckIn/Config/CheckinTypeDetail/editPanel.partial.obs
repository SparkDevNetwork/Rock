    const startTemplate = ref(props.modelValue?.bag?.displaySettings?.startTemplate ?? "");
    const familySelectTemplate = ref(props.modelValue?.bag?.displaySettings?.familySelectTemplate ?? "");
    const personSelectTemplate = ref(props.modelValue?.bag?.displaySettings?.personSelectTemplate ?? "");
    const successTemplateOverrideDisplayMode = ref(props.modelValue?.bag?.displaySettings?.successTemplateOverrideDisplayMode ?? "");
    const successTemplate = ref(props.modelValue?.bag?.displaySettings?.successTemplate ?? "");

    // Registration Settings
