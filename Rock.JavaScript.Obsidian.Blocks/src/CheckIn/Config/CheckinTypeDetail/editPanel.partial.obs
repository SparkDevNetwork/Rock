                preventInactivePeople: preventInactivePeople.value,
                preventDuplicateCheckin: preventDuplicateCheckin.value,
            };

            const barcodeSettings: CheckInBarcodeSettingsBag = {
                codeAlphaLength: codeAlphaLength.value,
                codeAlphaNumericLength: codeAlphaNumericLength.value,
                codeNumericLength: codeNumericLength.value,
                codeRandom: codeRandom.value,
                reuseCode: reuseCode.value,
            };

            const searchSettings: CheckInSearchSettingsBag = {
                searchType: searchType.value,
                maxResults: maxResults.value,
                minPhoneLength: minPhoneLength.value,
                maxPhoneLength: maxPhoneLength.value,
                phoneSearchType: phoneSearchType.value,
            };

            const headerText: CheckInHeaderTextBag = {
                actionSelectHeaderTemplate: actionSelectHeaderTemplate.value,
                checkoutPersonSelectHeaderTemplate: checkoutPersonSelectHeaderTemplate.value,
                personSelectHeaderTemplate: personSelectHeaderTemplate.value,
                multiPersonSelectHeaderTemplate: multiPersonSelectHeaderTemplate.value,
                groupTypeSelectHeaderTemplate: groupTypeSelectHeaderTemplate.value,
                timeSelectHeaderTemplate: timeSelectHeaderTemplate.value,
                abilityLevelSelectHeaderTemplate: abilityLevelSelectHeaderTemplate.value,
                locationSelectHeaderTemplate: locationSelectHeaderTemplate.value,
                groupSelectHeaderTemplate: groupSelectHeaderTemplate.value,
            };

            // This should suffice for now, but if a conclusion is reached on how to handle bags with nested properties
            // this will warrant a second look.
            setPropertiesBoxValue(newValue, "attributeValues", attributeValues.value);
            setPropertiesBoxValue(newValue, "description", description.value);
            setPropertiesBoxValue(newValue, "name", name.value);
            setPropertiesBoxValue(newValue, "iconCssClass", iconCssClass.value);
            setPropertiesBoxValue(newValue, "generalSettings", generalSettings);
            setPropertiesBoxValue(newValue, "barcodeSettings", barcodeSettings);
            setPropertiesBoxValue(newValue, "searchSettings", searchSettings);
            setPropertiesBoxValue(newValue, "displaySettings", displaySettings);
            setPropertiesBoxValue(newValue, "registrationSettings", registrationSettings);
            setPropertiesBoxValue(newValue, "headerText", headerText);
            setPropertiesBoxValue(newValue, "advancedSettings", advancedSettings);

            emit("update:modelValue", newValue);
        });

    // Watch for any changes to props that represent properties and then
    // automatically emit which property changed.
    watchPropertyChanges(propRefs, emit);
</script>
