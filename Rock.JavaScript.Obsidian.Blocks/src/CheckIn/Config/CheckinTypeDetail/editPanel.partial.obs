        // Header Text
        updateRefValue(actionSelectHeaderTemplate, props.modelValue?.bag?.headerText?.actionSelectHeaderTemplate ?? "");
        updateRefValue(checkoutPersonSelectHeaderTemplate, props.modelValue?.bag?.headerText?.checkoutPersonSelectHeaderTemplate ?? "");
        updateRefValue(personSelectHeaderTemplate, props.modelValue?.bag?.headerText?.personSelectHeaderTemplate ?? "");
        updateRefValue(multiPersonSelectHeaderTemplate, props.modelValue?.bag?.headerText?.multiPersonSelectHeaderTemplate ?? "");
        updateRefValue(groupTypeSelectHeaderTemplate, props.modelValue?.bag?.headerText?.groupTypeSelectHeaderTemplate ?? "");
        updateRefValue(timeSelectHeaderTemplate, props.modelValue?.bag?.headerText?.timeSelectHeaderTemplate ?? "");
        updateRefValue(abilityLevelSelectHeaderTemplate, props.modelValue?.bag?.headerText?.abilityLevelSelectHeaderTemplate ?? "");
        updateRefValue(locationSelectHeaderTemplate, props.modelValue?.bag?.headerText?.locationSelectHeaderTemplate ?? "");
        updateRefValue(groupSelectHeaderTemplate, props.modelValue?.bag?.headerText?.groupSelectHeaderTemplate ?? "");

        // Display Settings
        updateRefValue(hidePhotos, props.modelValue?.bag?.displaySettings?.hidePhotos ?? false);
        updateRefValue(startTemplate, props.modelValue?.bag?.displaySettings?.startTemplate ?? "");
        updateRefValue(familySelectTemplate, props.modelValue?.bag?.displaySettings?.familySelectTemplate ?? "");
        updateRefValue(personSelectTemplate, props.modelValue?.bag?.displaySettings?.personSelectTemplate ?? "");
        updateRefValue(successTemplateOverrideDisplayMode, props.modelValue?.bag?.displaySettings?.successTemplateOverrideDisplayMode ?? "");
        updateRefValue(successTemplate, props.modelValue?.bag?.displaySettings?.successTemplate ?? "");

        // Registration Settings
        updateRefValue(registrationDefaultPersonConnectionStatus, props.modelValue?.bag?.registrationSettings?.registrationDefaultPersonConnectionStatus ?? {});
        updateRefValue(registrationDisplayAlternateIdFieldForAdults, props.modelValue?.bag?.registrationSettings?.registrationDisplayAlternateIdFieldForAdults ?? false);
