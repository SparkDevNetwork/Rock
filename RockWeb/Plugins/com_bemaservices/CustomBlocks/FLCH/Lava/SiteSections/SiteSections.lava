        <div class="bg-gray-light">
            <div class="container container-padding">
               <div class="row">
                    <div class="col-md-offset-1 col-md-10 text-center margin-b-lg">
                        <h1 class="">Section 18 Group Contact</h1>                
                    </div>
                </div> 
        
           {% group where:'Id == 688' %} <!-- Set Group Id -->

                {% for group in groupItems %}

                    <div class="row row-eq-height">
                    
                    {% groupmember where:'GroupId == {{ group.Id }}' sort'LastName, FirstName, desc' %}
                    
                        {% for groupmember in groupmemberItems %}
                        
                        {% assign thisPerson = groupmember.Person %}

                            <div class="col-xs-12 col-md-3 text-center">
                                <div class="align-top p-3">
                                    <img class="img-circle staff-image" src="{{ thisPerson.PhotoUrl }}&maxwidth=150&maxheight=150" style="width: 150px; height=150px" alt="{{ thisPerson.FullName }}">
                                </div>

                                
                                    <div class="staff-name">{{ thisPerson.FullName }}</div>
                                    
                                    {% assign title = thisPerson | Attribute:'JobTitle' %}
                                            
                                            {% if title and title != '' %}
                                                <div class="staff-job-title">{{ thisPerson | Attribute:'JobTitle'  }}</div>
                                            {% endif %}
                                    
                        
                                <div class="staff-social text-center">
                                        
                                            {% assign phoneNumber = thisPerson | PhoneNumber:'Work' %}
                
                                            {% if phoneNumber != '' and phoneNumber != empty %}
                                                <a href="tel:{{ phoneNumber }}"><i class="fa fa-phone-alt"></i></a>&nbsp;
                                            {% endif %}
                                            
                                            <a href="/Contact/{{ thisPerson.PrimaryAlias.Guid }}/"><i class="fa fa-envelope"></i></a>
                                            
                                </div>
                            </div>
                            
                        {% endfor %}
                        
                    {% endgroupmember %}
                    
                    </div>
                    
                {% endfor %}
                
{% endgroup %}


            </div>
        </div>
    </section>
</div>


