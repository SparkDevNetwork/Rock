  </div><!-- Other Deductions Row -->
  <h3 width="100%" style="border-bottom: 1pt solid black;"> Employee Bi-Weekly Contribution Election </h3>
  <div class="row"> <!-- Contribution Row -->
  </br>
  <div class="col-xs-3"><!-- Contribution Legend -->
    <table width="100%">
      <tr>
        <td><p > </p></td>
      </tr>
      {% for contribution in Contributions %}
      <tr>
        <td><p><b> {{contribution.FinancialAccount}}</b></p></td>
      </tr>
      {% endfor %}
      <tr>
        <td><p>Total contributions elected per pay period</p></td>
      </tr>
      </table>
    </br>
    <table>
      <tr>
        <td><p style="text-align:right;"><b>Total Bi-weekly Wage x 10%</b></p></td>
      </tr>
    </table>
  </div><!-- Contribution Legend -->
  <div class="col-xs-3"> <!-- Previous Year Contribution -->
    {% assign totalPreviousAmount = 0 %}
    {% assign totalPreviousPercent = 0 %}
    <table width="100%">
      <tr>
        <td style="text-align:center;" width="50%"><p style="font-size:10pt;"> Fixed</p></td>
        <td style="text-align:center;" width="50%"><p style="font-size:10pt;"> % of Gross</p></td>
      </tr>
      {% for contribution in Contributions %}
      <tr>
        <td style="border:1pt solid black;text-align:right;">
          {% if contribution.PreviousFixed %}
            {% assign previousAmount = contribution.PreviousAmount %}
          {% else %}
		  {% if totalPreviousBiWeeklyPay and totalPreviousBiWeeklyPay != empty %}
            {% assign previousAmount = totalPreviousBiWeeklyPay | Times:contribution.PreviousAmount | DividedBy:100,4 %}
			{% endif %}
          {% endif %}
		  {% if previousAmount and previousAmount != empty %}
          <p{% if contribution.PreviousFixed != true %} style="color:darkgray !important;"{% endif %}>{{ previousAmount | FormatAsCurrency }}</p>
          {% assign totalPreviousAmount = totalPreviousAmount | Plus:previousAmount %}
		  {% endif %}
        </td>
        <td style="border:1pt solid black;text-align:right;">
          {% if contribution.PreviousFixed %}
		  {% if totalPreviousBiWeeklyPay and totalPreviousBiWeeklyPay != empty %}
            {% assign previousPercent = contribution.PreviousAmount | DividedBy:totalPreviousBiWeeklyPay,4 %}
			{% endif %}
          {% else %}
            {% assign previousPercent = contribution.PreviousAmount | DividedBy:100,4 %}
          {% endif %}
		  {% if previousPercent and previousPercent != empty %}
          <p{% if contribution.PreviousFixed %} style="color:darkgray !important;"{% endif %}>{{previousPercent | Format:'P' }}</p>
          {% assign totalPreviousPercent = totalPreviousPercent | Plus:previousPercent %}
		  {% endif %}
        </td>
      </tr>
      {% endfor %}
      <tr>
        <td style="border:2pt solid black;text-align:right;"><p>{{ totalPreviousAmount | FormatAsCurrency }}</p></td>
        <td style="border:2pt solid black;text-align:right;"><p>{{totalPreviousPercent | Format:'P' }}</p></td>
      </tr>
    </table>
  </br>
    <table width="100%">
      <tr>
        <td style="text-align:right;" width="50%"><p>{{ totalPreviousBiWeeklyPay | DividedBy:10,4 | FormatAsCurrency }}</p></td>
        <td style="text-align:right;" width="50%"><p> </p></td>
      </tr>
    </table>
</div> <!-- Previous Year Contribution -->
<div class="col-xs-3"> <!-- Current Year Contribution -->
  {% assign totalCurrentAmount = 0 %}
  {% assign totalCurrentPercent = 0 %}
  <table width="100%">
    <tr>
      <td style="text-align:center;" width="50%"><p style="font-size:10pt;"> Fixed</p></td>
      <td style="text-align:center;" width="50%"><p style="font-size:10pt;"> % of Gross</p></td>
    </tr>
    {% for contribution in Contributions %}
    <tr>
      <td style="border:1pt solid black;text-align:right;">
        {% if contribution.CurrentFixed %}
          {% assign CurrentAmount = contribution.CurrentAmount %}
        {% else %}
		{% if totalCurrentBiWeeklyPay and totalCurrentBiWeeklyPay != empty %}
          {% assign CurrentAmount = totalCurrentBiWeeklyPay | Times:contribution.CurrentAmount | DividedBy:100,4 %}
		  {% endif %}
        {% endif %}
		{% if CurrentAmount and CurrentAmount != empty %}
        <p{% if contribution.CurrentFixed != true %} style="color:darkgray !important;"{% endif %}>{{ CurrentAmount | FormatAsCurrency }}</p>
        {% assign totalCurrentAmount = totalCurrentAmount | Plus:CurrentAmount %}
		{% endif %}
      </td>
      <td style="border:1pt solid black;text-align:right;">
        {% if contribution.CurrentFixed %}
		{% if totalCurrentBiWeeklyPay and totalCurrentBiWeeklyPay != empty %}
          {% assign CurrentPercent = contribution.CurrentAmount | DividedBy:totalCurrentBiWeeklyPay,4 %}
		  {% endif %}
        {% else %}
          {% assign CurrentPercent = contribution.CurrentAmount | DividedBy:100,4 %}
        {% endif %}
		{% if CurrentPercent and CurrentPercent != empty %}
        <p{% if contribution.CurrentFixed %} style="color:darkgray !important;"{% endif %}>{{CurrentPercent | Format:'P' }}</p>
        {% assign totalCurrentPercent = totalCurrentPercent | Plus:CurrentPercent %}
		{% endif %}
      </td>
    </tr>
    {% endfor %}
    <tr>
      <td style="border:2pt solid black;text-align:right;"><p>{{ totalCurrentAmount | FormatAsCurrency }}</p></td>
      <td style="border:2pt solid black;text-align:right;"><p>{{totalCurrentPercent | Format:'P' }}</p></td>
    </tr>
  </table>
</br>
  <table width="100%">
    <tr>
      <td style="text-align:right;" width="50%"><p>{{ totalCurrentBiWeeklyPay | DividedBy:10,4 | FormatAsCurrency }}</p></td>
      <td style="text-align:right;" width="50%"><p> </p></td>
    </tr>
  </table>
</div> <!-- Current Year Contribution -->
<div class="col-xs-3"> <!-- Current Year Contribution -->
  {% assign totalCurrentAmount = 0 %}
  {% assign totalCurrentPercent = 0 %}
  <table width="100%">
    <tr>
      <td style="text-align:center;" width="50%"><p style="font-size:10pt;"> Fixed</p></td>
      <td style="text-align:center;" width="50%"><p style="font-size:10pt;"> % of Gross</p></td>
    </tr>
    {% for contribution in Contributions %}
    <tr>
      <td style="border:1pt solid black;text-align:right;">
        {% if contribution.CurrentFixed %}
          {% assign CurrentAmount = contribution.CurrentAmount %}
        {% else %}
		{% if totalFutureBiWeeklyPay and totalFutureBiWeeklyPay != empty %}
          {% assign CurrentAmount = totalFutureBiWeeklyPay | Times:contribution.CurrentAmount | DividedBy:100,4 %}
		  {% endif %}
        {% endif %}
		{% if CurrentAmount and CurrentAmount != empty %}
        <p{% if contribution.CurrentFixed != true %} style="color:darkgray !important;"{% endif %}>{{ CurrentAmount | FormatAsCurrency }}</p>
        {% assign totalCurrentAmount = totalCurrentAmount | Plus:CurrentAmount %}
		{% endif %}
      </td>
      <td style="border:1pt solid black;text-align:right;">
        {% if contribution.CurrentFixed %}
		{% if totalFutureBiWeeklyPay and totalFutureBiWeeklyPay != empty %}
          {% assign CurrentPercent = contribution.CurrentAmount | DividedBy:totalFutureBiWeeklyPay,4 %}
		  {% endif %}
        {% else %}
          {% assign CurrentPercent = contribution.CurrentAmount | DividedBy:100,4 %}
        {% endif %}
		{% if CurrentPercent and CurrentPercent != empty %}
        <p{% if contribution.CurrentFixed %} style="color:darkgray !important;"{% endif %}>{{CurrentPercent | Format:'P' }}</p>
        {% assign totalCurrentPercent = totalCurrentPercent | Plus:CurrentPercent %}
		{% endif %}
      </td>
    </tr>
    {% endfor %}
    <tr>
      <td style="border:2pt solid black;text-align:right;"><p>{{ totalCurrentAmount | FormatAsCurrency }}</p></td>
      <td style="border:2pt solid black;text-align:right;"><p>{{totalCurrentPercent | Format:'P' }}</p></td>
    </tr>
  </table>
</br>
  <table width="100%">
    <tr>
      <td style="text-align:right;" width="50%"><p>{{ totalFutureBiWeeklyPay | DividedBy:10,4 | FormatAsCurrency }}</p></td>
      <td style="text-align:right;" width="50%"><p> </p></td>
    </tr>
  </table>
</div> <!-- Current Year Contribution -->
  </div>  <!-- Contribution Row -->
</br>
</br>
  <div class="row"> <!-- Signature Row -->
    <div class="col-xs-5">
      <p style="border-bottom:1pt solid black" width="100%"> </p>
      <b> Employee Signature, Date </b>
    </div>
    <div class="col-xs-2">
    </div>
    <div class="col-xs-5">
      <p style="border-bottom:1pt solid black" width="100%"> </p>
      <b> Processed By, Date</b>
    </div>
  </div> <!-- Signature Row -->
<p>This employee payroll deduction sheet shall not be construed as a contract.  Central Christian Church, Arizona reserves the right to make changes as it deems appropriate.</p>
</div>
</body>
