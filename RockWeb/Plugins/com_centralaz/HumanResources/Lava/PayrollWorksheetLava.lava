<html>
  <head>
    <title>{{ 'Now' | Date:'MMddyyyy' }}_ContributionStatements_Chapter_{{ ChapterNumber }}_of_{{ ChapterSize }}.html</title>
    <link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
      <link rel="stylesheet" type="text/css" media="print" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
  </head>
  <body>
    <style>
      p {
        white-space: pre;
      }

      #printOnly {
      display: none;
      }

      @media print {
      #printOnly {
      display: block;
      }
      }
    </style>
    <!-- Global Variable assignment section -->
    {% assign employmentStatus = Person | Attribute:'HREmploymentStatus' %}
  {% if employmentStatus contains "Full-time" %}
    {% assign fullTime = true %}
  {% else %}
    {% assign fullTime = false %}
  {% endif %}

  {% if employmentStatus contains "Hourly" %}
    {% assign hourly = true %}
  {% else %}
    {% assign hourly = false %}
  {% endif %}

  {% assign approvedHours = Person | Attribute:'Arena-7-240' %}
    <!-- Global Variable assignment section -->
    <div class="container">
		<div class="row"> <!-- Header / Attribute Row -->
			<div class="col-xs-6"> <!-- Header Column -->
			<div class="row">
        <div class="col-xs-12">
  				<h2>
  					Central Christian Church </br>
  					{% if !fulltime %}PT {%endif%}{% if hourly %}(Hourly) {%endif%} Employee Payroll Information </br>
  					Deduction Authorization Sheet </br>
  				</h2>
        </div>
			</div>
			<div class="row">
				<div class="col-xs-6">
          <p style="border-bottom:1pt solid black" width="100%">{{Person.LastName}}, {{Person.FirstName}}</p>
          <b> Employee Name </b>
				</div>
        <div class="col-xs-6">
          <div class="row">
    				<div class="col-xs-4">
              Department:
            </div>
            <div class="col-xs-8">
              <p style="border-bottom:1pt solid black" width="100%">{{ Person | Attribute:'HRDepartment'}}</p> <!-- Department Attribute -->
            </div>
          </div>
          <div class="row">
      				<div class="col-xs-4">
                Title:
              </div>
              <div class="col-xs-8">
                <p style="border-bottom:1pt solid black" width="100%">{{Person | Attribute:'HRJobTitle'}}</p> <!-- Job Title Attribute -->
              </div>
            </div>
          </div>
        </div>
			</div> <!-- Header Column -->
			<div class="col-xs-6" style="border:2pt solid black"> <!-- Attribute Column -->
        <div class="row"><!-- Attribute Row 1 -->
          <div class="col-xs-2">
            Start Date:
          </div>
          <div class="col-xs-4">
            <p style="border-bottom:1pt solid black" width="100%">{{Person | Attribute:'HRHireDate'}} </p> <!-- Start Date Attribute -->
          </div>
          <div class="col-xs-2">
            Birth Date:
          </div>
          <div class="col-xs-4">
            <p style="border-bottom:1pt solid black" width="100%">{{Person.BirthDate | Date:'M/d/yyyy'}} </p> <!-- Birth Date Attribute -->
          </div>
        </div><!-- Attribute Row 1 -->
        <div class="row"><!-- Attribute Row 2 -->
          <div class="col-xs-4">
            Federal Filing Status:
        </div>
        <div class="col-xs-8">
          <p style="border-bottom:1pt solid black" width="100%">{{Person | Attribute:'Arena-23-213'}}</p> <!-- Federal Filing Status Attribute -->
        </div>
      </div> <!-- Attribute Row 2 -->
      <div class="row"> <!-- Attribute Row 3 -->
        <div class="col-xs-2">
          Exemptions:
        </div>
        <div class="col-xs-2">
            <p style="border-bottom:1pt solid black" width="100%">{{Person | Attribute:'Arena-23-214'}} </p> <!-- Federal Exemptions Attribute -->
        </div>
        <div class="col-xs-2">
          Exempt:
        </div>
        <div class="col-xs-2">
            <p style="border-bottom:1pt solid black" width="100%">{{Person | Attribute:'Arena-23-215'}} </p> <!-- Federal Exempt Attribute -->
        </div>
        <div class="col-xs-2">
          Add'l $
        </div>
        <div class="col-xs-2">
            <p style="border-bottom:1pt solid black" width="100%">{{Person | Attribute:'Arena-23-216'}} </p> <!-- Federal Additional $ Attribute -->
        </div>
      </div><!-- Attribute Row 3 -->
      <div class="row"><!-- Attribute Row 4 -->
        <div class="col-xs-2">
          <p style="font-size:10pt;">State W/H:</p>
        </div>
        <div class="col-xs-2">
            <p style="border-bottom:1pt solid black" width="100%">{{Person | Attribute:'Arena-23-217' | Format:'#0.0000' }} </p> <!-- State Withholding Attribute -->
        </div>
        <div class="col-xs-2">
          Exempt:
        </div>
        <div class="col-xs-2">
            <p style="border-bottom:1pt solid black" width="100%">{{Person | Attribute:'Arena-23-218'}} </p> <!-- State Exempt Attribute -->
        </div>
        <div class="col-xs-2">
          Add'l $
        </div>
        <div class="col-xs-2">
            <p style="border-bottom:1pt solid black" width="100%">{{Person | Attribute:'Arena-23-219'}} </p> <!-- State Additional $ Attribute -->
        </div>
      </div><!-- Attribute Row 4 -->
      <div class="row"><!-- Attribute Row 5 -->
        <div class="col-xs-3">
          <p style="font-size:10pt;">Vacation Days/Hrs:</p>
        </div>
        <div class="col-xs-3">
          <p style="border-bottom:1pt solid black" width="100%">{{Person | Attribute:'Arena-24-220' | Format:'#0.0000' }} </p> <!-- Vacation Hours Attribute -->
        </div>
        <div class="col-xs-3">
          Sick Days/Hrs:
        </div>
        <div class="col-xs-3">
          <p style="border-bottom:1pt solid black" width="100%">{{Person | Attribute:'Arena-24-221' | Format:'#0.0000' }} </p> <!-- Sick Hours Attribute -->
        </div>
      </div><!-- Attribute Row 5 -->
      <div class="row"><!-- Attribute Row 6 -->
        <div class="col-xs-4">
          Health Coverage Type:
        </div>
        <div class="col-xs-8">
          <p style="border-bottom:1pt solid black" width="100%">{{Person | Attribute:'Arena-7-207'}} </p> <!-- Health Coverage Type Attribute -->
        </div>
      </div><!-- Attribute Row 6 -->
      <div class="row"><!-- Attribute Row 7 -->
        <div class="col-xs-4">
          Dental Coverage Type:
        </div>
        <div class="col-xs-8">
          <p style="border-bottom:1pt solid black" width="100%">{{Person | Attribute:'Arena-7-206'}} </p> <!-- Dental Coverage Type Attribute -->
        </div>
      </div><!-- Attribute Row 7 -->
		</div> <!-- Attribute Column -->
	</div> <!-- Header / Attribute Row -->
  <div class="row"> <!-- Wages Row -->
    <div class="col-xs-3"> <!-- Wages legend column -->
      <table style="width:100%;font-weight:bold;">
        <tr>
          <td width="50%"><p> </p></td>
          <td width="50%"><p> </p></td>
        </tr>
        <tr>
          <td>Wage</td>
          <td><p> </p></td>
        </tr>
        {% if hourly == false %}
        <tr>
          <td>Housing</td>
          <td><p> </p></td>
        </tr>
        {% endif %}
        {% if hourly %}
        <tr>
          <td>Hours approved to work per p/p</td>
          <td><p> </p></td>
        </tr>
        {% endif %}
        <tr>
          <td>Total</td>
          <td><p> </p></td>
        </tr>
        <tr>
          <td>Bi-Weekly Pay</td>
          <td><p> </p></td>
        </tr>
        <tr>
          <td><p> </p></td>
          <td><p> </p></td>
        </tr>
        {% if fullTime %}
        <tr>
          <td>403 b Plan Year</td>
          <td><p> </p></td>
        </tr>
        <tr>
          <td><p>Eligible 403b %</p></td>
          <td>Employee</td>
        </tr>
        <tr>
          <td><p> </p></td>
          <td>Employer</td>
        </tr>
        {% endif %}
      </table>
      {% if fullTime %}
      (Wage x % divided by 26 pay periods)
      {% endif %}
    </div> <!-- Wages legend column -->
    <div class="col-xs-3"> <!-- Wages Last Year column -->
      <table style="width:100%">
        <tr>
          <td width="50%"><b>{{Year | Minus:1 }}/{{Year}}</td>
            <td width="50%"><p> </p></td>
        </tr>
        <tr>
          <td style="border:1pt solid black;text-align:right;"><p> {{PreviousSalary.Amount | FormatAsCurrency }}</p></td>
          <td><p> </p></td>
        </tr>
        {% if hourly == false %}
        <tr>
          <td style="border:1pt solid black;text-align:right;"><p> {{PreviousSalary.HousingAllowance | FormatAsCurrency }}</p></td>
          <td><p> </p></td>
        </tr>
        {% endif %}
        {% if hourly %}
        <tr>
          <td style="border:1pt solid black;text-align:right;">{{ approvedHours }}</td> <!-- Hours approved to work per p/p attribute -->
          <td><p> </p></td>
        </tr>
        {% endif %}
        <tr>
          <td style="border:2pt solid black;text-align:right;">
            {% if hourly %}
              {% assign previousTotalAmount = PreviousSalary.Amount | Times:approvedHours | Times:26 %}
            {% else %}
              {% assign previousTotalAmount = PreviousSalary.Amount | Plus:PreviousSalary.HousingAllowance  %}
            {% endif %}
            <p>{{ previousTotalAmount | FormatAsCurrency}}</p>
          </td>
          <td><p> </p></td>
        </tr>
        <tr>{% assign totalPreviousBiWeeklyPay = previousTotalAmount | DividedBy:26,4 %}
          <td style="border:1pt solid black;text-align:right;"><p> {{ totalPreviousBiWeeklyPay | FormatAsCurrency }}</p></td>
          <td><p> </p></td>
        </tr>
        <tr>
          <td><p> </p></td>
          <td><p> </p></td>
        </tr>
        {% if fullTime %}
        {% assign employmentPreviousYears = 0 | AsInteger %}
        {% assign employmentPreviousYearsAttribute = Person | Attribute:'Arena-22-212' %}
        <tr> {% if employmentPreviousYearsAttribute and employmentPreviousYearsAttribute != empty %}{% assign employmentPreviousYears = employmentPreviousYearsAttribute | Minus:1 | Ceiling %}{% endif %}
            {% if employmentPreviousYears < 0 %}{% assign employmentPreviousYears = 0 | AsInteger %}{% endif %}
          <td style="border:1pt solid black;text-align:center;"><p><b>{{ employmentPreviousYears | ToString | NumberToOrdinal }}</b></p></td>
          <td><p> </p></td>
        </tr>
        <tr> {% if employmentPreviousYears == 1 %}{% assign employeePreviousPercent = 0.04 %}{% elseif employmentPreviousYears == 2 %}{% assign employeePreviousPercent = 0.08 %}{% elseif employmentPreviousYears >= 3 %}{% assign employeePreviousPercent = 0.12 %}{% else %}{% assign employeePreviousPercent = 0.00 %}{% endif %}
          <td style="border:1pt solid black;text-align:right;"><p> {{ totalPreviousBiWeeklyPay | Times:employeePreviousPercent | FormatAsCurrency }}</p></td>
          <td style="border:1pt solid black;text-align:right;"><p> {{employeePreviousPercent | Format:"P"}}</p></td>
        </tr>
        <tr> {% if employmentPreviousYears == 1 %}{% assign employerPreviousPercent = 0.02 %}{% elseif employmentPreviousYears == 2 %}{% assign employerPreviousPercent = 0.04 %}{% elseif employmentPreviousYears == 3 %}{% assign employerPreviousPercent = 0.06 %}{% elseif employmentPreviousYears == 4 %}{% assign employerPreviousPercent = 0.08 %}{% elseif employmentPreviousYears == 5 %}{% assign employerPreviousPercent = 0.10 %}{% elseif employmentPreviousYears >= 6 %}{% assign employerPreviousPercent = 0.12 %}{% else %}{% assign employerPreviousPercent = 0.00 %}{% endif %}
          <td style="border:1pt solid black;text-align:right;"><p> {{ totalPreviousBiWeeklyPay | Times:employerPreviousPercent | FormatAsCurrency }}</p></td>
          <td style="border:1pt solid black;text-align:right;"><p> {{employerPreviousPercent | Format:"P"}}</p></td>
        </tr>
        {%endif%}
      </table>
    </div> <!-- Wages Last Year column -->
    <div class="col-xs-3"> <!-- Wages Current Year column -->
      <table style="width:100%">
        <tr>
          <td width="50%"><b>{{ Year }}/{{Year | Plus:1 }}</td>
            <td width="50%"><p> </p></td>
        </tr>
        <tr>
          <td style="border:1pt solid black;text-align:right;"><p> {{CurrentSalary.Amount | FormatAsCurrency }}</p></td>
          <td><p> </p></td>
        </tr>
        {% if hourly == false %}
        <tr>
          <td style="border:1pt solid black;text-align:right;"><p> {{CurrentSalary.HousingAllowance | FormatAsCurrency }}</p></td>
          <td><p> </p></td>
        </tr>
        {% endif %}
        {% if hourly %}
        <tr>
          <td style="border:1pt solid black;text-align:right;">{{ approvedHours }}</td> <!-- Hours approved to work per p/p attribute -->
          <td><p> </p></td>
        </tr>
        {% endif %}
        <tr>
          <td style="border:2pt solid black;text-align:right;">
            {% if hourly %}
              {% assign currentTotalAmount = CurrentSalary.Amount | Times:approvedHours | Times:26 %}
            {% else %}
              {% assign currentTotalAmount = CurrentSalary.Amount | Plus:CurrentSalary.HousingAllowance  %}
            {% endif %}
            <p>{{ currentTotalAmount | FormatAsCurrency}}</p>
          </td>
          <td><p> </p></td>
        </tr>
        <tr>{% assign totalCurrentBiWeeklyPay = currentTotalAmount | DividedBy:26,4 %}
          <td style="border:1pt solid black;text-align:right;"><p> {{ totalCurrentBiWeeklyPay | FormatAsCurrency }}</p></td>
          <td><p> </p></td>
        </tr>
        <tr>
          <td><p> </p></td>
          <td><p> </p></td>
        </tr>
        {% if fullTime %}
        {% assign employmentCurrentYears = 0 | AsInteger %}
        {% assign employmentCurrentYearsAttribute = Person | Attribute:'Arena-22-212' %}
        <tr> {% if employmentCurrentYearsAttribute and employmentCurrentYearsAttribute != empty %}{% assign employmentCurrentYears = employmentCurrentYearsAttribute | Ceiling %}{% endif %}
          <td style="border:1pt solid black;text-align:center;"><p> <b>{{ employmentCurrentYears | ToString | NumberToOrdinal }}</b></p></td>
          <td><p> </p></td>
        </tr>
        <tr> {% if employmentCurrentYears == 1 %}{% assign employeeCurrentPercent = 0.04 %}{% elseif employmentCurrentYears == 2 %}{% assign employeeCurrentPercent = 0.08 %}{% elseif employmentCurrentYears >= 3 %}{% assign employeeCurrentPercent = 0.12 %}{% else %}{% assign employeeCurrentPercent = 0.00 %}{% endif %}
          <td style="border:1pt solid black;text-align:right;"><p> {{ totalCurrentBiWeeklyPay | Times:employeeCurrentPercent | FormatAsCurrency }}</p></td>
          <td style="border:1pt solid black;text-align:right;"><p> {{employeeCurrentPercent | Format:"P"}}</p></td>
        </tr>
        <tr> {% if employmentCurrentYears == 1 %}{% assign employerCurrentPercent = 0.02 %}{% elseif employmentCurrentYears == 2 %}{% assign employerCurrentPercent = 0.04 %}{% elseif employmentCurrentYears == 3 %}{% assign employerCurrentPercent = 0.06 %}{% elseif employmentCurrentYears == 4 %}{% assign employerCurrentPercent = 0.08 %}{% elseif employmentCurrentYears == 5 %}{% assign employerCurrentPercent = 0.10 %}{% elseif employmentCurrentYears >= 6 %}{% assign employerCurrentPercent = 0.12 %}{% else %}{% assign employerCurrentPercent = 0.00 %}{% endif %}
          <td style="border:1pt solid black;text-align:right;"><p> {{ totalCurrentBiWeeklyPay | Times:employerCurrentPercent | FormatAsCurrency }}</p></td>
          <td style="border:1pt solid black;text-align:right;"><p> {{employerCurrentPercent | Format:"P"}}</p></td>
        </tr>
        {%endif%}
      </table>
    </div> <!-- Wages Current Year column -->
    <div class="col-xs-3"> <!-- Wages Future Year column -->
      <table style="width:100%">
        <tr>
          <td width="50%"><b>{{ Year | Plus:1 }}/{{Year | Plus:2 }}</td>
            <td width="50%"><p> </p></td>
        </tr>
        <tr>
          <td style="border:1pt solid black;text-align:right;"><p> {{FutureSalary.Amount | FormatAsCurrency }}</p></td>
          <td><p> </p></td>
        </tr>
        {% if hourly == false %}
        <tr>
          <td style="border:1pt solid black;text-align:right;"><p> {{FutureSalary.HousingAllowance | FormatAsCurrency }}</p></td>
          <td><p> </p></td>
        </tr>
        {% endif %}
        {% if hourly %}
        <tr>
          <td style="border:1pt solid black;text-align:right;">{{ approvedHours }}</td> <!-- Hours approved to work per p/p attribute -->
          <td><p> </p></td>
        </tr>
        {% endif %}
        <tr>
          <td style="border:2pt solid black;text-align:right;">
            {% if hourly %}
              {% assign futureTotalAmount = FutureSalary.Amount | Times:approvedHours | Times:26 %}
            {% else %}
              {% assign futureTotalAmount = FutureSalary.Amount | Plus:FutureSalary.HousingAllowance %}
            {% endif %}
            <p>{{ futureTotalAmount | FormatAsCurrency}}</p>
          </td>
          <td><p> </p></td>
        </tr>
        <tr>{% assign totalFutureBiWeeklyPay = futureTotalAmount | DividedBy:26,4 %}
          <td style="border:1pt solid black;text-align:right;"><p> {{ totalFutureBiWeeklyPay | FormatAsCurrency }}</p></td>
          <td><p> </p></td>
        </tr>
        <tr>
          <td><p> </p></td>
          <td><p> </p></td>
        </tr>
        {% if fullTime %}
        <tr> {% assign employmentFutureYears = Person | Attribute:'Arena-22-212' | Plus: 1 | Ceiling %}
          <td style="border:1pt solid black;text-align:center;"><p> <b>{{ employmentFutureYears | ToString | NumberToOrdinal }}</b></p></td>
          <td><p> </p></td>
        </tr>
        <tr> {% if employmentFutureYears == 1 %}{% assign employeeFuturePercent = 0.04 %}{% elseif employmentFutureYears == 2 %}{% assign employeeFuturePercent = 0.08 %}{% elseif employmentFutureYears >= 3 %}{% assign employeeFuturePercent = 0.12 %}{% else %}{% assign employeeFuturePercent = 0.00 %}{% endif %}
          <td style="border:1pt solid black;text-align:right;"><p> {{ totalFutureBiWeeklyPay | Times:employeeFuturePercent | FormatAsCurrency }}</p></td>
          <td style="border:1pt solid black;text-align:right;"><p> {{employeeFuturePercent | Format:"P"}}</p></td>
        </tr>
        <tr> {% if employmentFutureYears == 1 %}{% assign employerFuturePercent = 0.02 %}{% elseif employmentFutureYears == 2 %}{% assign employerFuturePercent = 0.04 %}{% elseif employmentFutureYears == 3 %}{% assign employerFuturePercent = 0.06 %}{% elseif employmentFutureYears == 4 %}{% assign employerFuturePercent = 0.08 %}{% elseif employmentFutureYears == 5 %}{% assign employerFuturePercent = 0.10 %}{% elseif employmentFutureYears >= 6 %}{% assign employerFuturePercent = 0.12 %}{% else %}{% assign employerFuturePercent = 0.00 %}{% endif %}
          <td style="border:1pt solid black;text-align:right;"><p> {{ totalFutureBiWeeklyPay | Times:employerFuturePercent | FormatAsCurrency }}</p></td>
          <td style="border:1pt solid black;text-align:right;"><p> {{employerFuturePercent | Format:"P"}}</p></td>
        </tr>
        {% endif %}
      </table>
    </div> <!-- Wages Future Year column -->
  </div><!-- Wages Row -->
  <h3 width="100%" style="border-bottom: 1pt solid black;"> Employee 403b Bi-Weekly Deduction Election </h3>
  <div class="row"> <!-- Actual Retirement Deduction Row -->
    <div class="col-xs-3">
      <table width="100%">
        <tr>
          <td><p > </p></td>
        </tr>
        {% for retirementFund in RetirementFunds %}
        <tr>
          <td><p><b> {{retirementFund.FinancialAccount}}</b></p></td>
        </tr>
        {% endfor %}
        </table>
      </br>
      <table>
        <tr>
          <td ><p><b>Total</b></p></td>
        </tr>
      </table>
      {% if fullTime %}
      </br>
      <table width="100%">
        <tr>
          <td width="25%"><p> </p></td>
          <td width="25%"><p style="text-align:right;color:darkgray !important;">Eligibility</p></td>
          <td width="25%"><p> </p></td>
          <td width="25%"><p> </p></td>
        </tr>
        {% if employmentPreviousYears <= 1 %}
        <tr>
          <td width="25%"><p> </p></td>
          <td width="25%"><p> </p></td>
          <td width="25%"><p> </p></td>
          <td width="25%"><p style="text-align:right;color:darkgray !important;">1st Year</p></td>
        </tr>
        {% endif %}
        {% if employmentPreviousYears <= 2 %}
        <tr>
          <td width="25%"><p> </p></td>
          <td width="25%"><p> </p></td>
          <td width="25%"><p> </p></td>
          <td width="25%"><p style="text-align:right;color:darkgray !important;">2nd Year</p></td>
        </tr>
        {% endif %}
        {% if employmentPreviousYears <= 3 %}
        <tr>
          <td width="25%"><p> </p></td>
          <td width="25%"><p> </p></td>
          <td width="25%"><p> </p></td>
          <td width="25%"><p style="text-align:right;color:darkgray !important;">3rd Year</p></td>
        </tr>
        {% endif %}
        {% if employmentPreviousYears <= 4 %}
        <tr>
          <td width="25%"><p> </p></td>
          <td width="25%"><p> </p></td>
          <td width="25%"><p> </p></td>
          <td width="25%"><p style="text-align:right;color:darkgray !important;">4th Year</p></td>
        </tr>
        {% endif %}
        {% if employmentPreviousYears <= 5 %}
        <tr>
          <td width="25%"><p> </p></td>
          <td width="25%"><p> </p></td>
          <td width="25%"><p> </p></td>
          <td width="25%"><p style="text-align:right;color:darkgray !important;">5th Year</p></td>
        </tr>
        {% endif %}
        <tr>
          <td width="25%"><p> </p></td>
          <td width="25%"><p> </p></td>
          <td width="25%"><p> </p></td>
          <td width="25%"><p style="text-align:right;color:darkgray !important;">6th Year</p></td>
        </tr>
      </table>
      {% endif %}
    </div>
    <div class="col-xs-3"> <!-- Previous Year Retirement Deduction -->
      {% assign totalPreviousEmployeeAmount = 0 %}
      {% assign totalPreviousEmployeePercent = 0 %}
      {% assign totalPreviousEmployerAmount = 0 %}
      {% assign totalPreviousEmployerPercent = 0 %}
      <table width="100%">
        <tr>
          <td style="border:1pt solid black;text-align:center;" width="25%"><p style="font-size:10pt;"> Employee</p></td>
          <td style="border:1pt solid black;text-align:center;" width="25%"><p>%</p></td>
          <td style="border:1pt solid black;text-align:center;" width="25%"><p style="font-size:10pt;"> {% if fullTime %}Employer{% endif %}</p></td>
          <td style="border:1pt solid black;text-align:center;" width="25%"><p> {% if fullTime %}%{% endif %}</p></td>
        </tr>
        {% for retirementFund in RetirementFunds %}
        <tr>
          <td style="border:1pt solid black;text-align:right;">
            {% if retirementFund.PreviousFixed %}
              {% assign previousEmployeeAmount = retirementFund.PreviousEmployeeAmount %}
            {% else %}
			{% if totalPreviousBiWeeklyPay and totalPreviousBiWeeklyPay != empty %}
              {% assign previousEmployeeAmount = totalPreviousBiWeeklyPay | Times:retirementFund.PreviousEmployeeAmount | DividedBy:100,4 %}
			  {% endif %}
            {% endif %}
			{% if previousEmployeeAmount and previousEmployeeAmount != empty %}
            <p{% if retirementFund.PreviousFixed != true %} style="color:darkgray !important;"{% endif %}>{{ previousEmployeeAmount | FormatAsCurrency }}</p>
            {% assign totalPreviousEmployeeAmount = totalPreviousEmployeeAmount | Plus:previousEmployeeAmount %}
			{% endif %}
          </td>
          <td style="border:1pt solid black;text-align:right;">
            {% if retirementFund.PreviousFixed %}
			{% if totalPreviousBiWeeklyPay and totalPreviousBiWeeklyPay != empty %}
              {% assign previousEmployeePercent = retirementFund.PreviousEmployeeAmount | DividedBy:totalPreviousBiWeeklyPay,4 %}
			  {% endif %}
            {% else %}
              {% assign previousEmployeePercent = retirementFund.PreviousEmployeeAmount | DividedBy:100,4 %}
            {% endif %}
			{% if previousEmployeePercent and previousEmployeePercent != empty %}
            <p{% if retirementFund.PreviousFixed %} style="color:darkgray !important;"{% endif %}>{{previousEmployeePercent | Format:'P' }}</p>
            {% assign totalPreviousEmployeePercent = totalPreviousEmployeePercent | Plus:previousEmployeePercent %}
			{% endif %}
          </td>
          <td style="border:1pt solid black;text-align:right;">
            {% if fullTime %}
              {% if retirementFund.PreviousFixed %}
                {% assign previousEmployerAmount = retirementFund.PreviousEmployerAmount %}
              {% else %}
			  {% if totalPreviousBiWeeklyPay and totalPreviousBiWeeklyPay != empty %}
                {% assign previousEmployerAmount = totalPreviousBiWeeklyPay | Times:retirementFund.PreviousEmployerAmount | DividedBy:100,4 %}
