{% for item in Items %}
{% assign detailImageGuid = item | Attribute:'DetailImage','RawValue' %}
{% if detailImageGuid != '' %}
<img alt="{{ item.Title }}" src="/GetImage.ashx?Guid={{ detailImageGuid }}" class="title-image img-responsive">
  {% endif %}
  <h1>{{ item.Title }}</h1>{{ item.Content }}
  {% endfor -%}
