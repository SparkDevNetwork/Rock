{% comment %} {% include '~~/Assets/Lava/_SiteNav.lava' %} {% endcomment %}

<header class="pjs-main-header" style="">
  <div class="pjs-inner">
    <div class="logo">
      <a href="https://www.willowcreek.org/">
        <img src="https://eaqmp884dxd.exactdn.com/wp-content/themes/wc2021/images/logo-willow.svg" alt="Willow logo"
          width="234" height="61">
      </a>
    </div>
    <nav class="header-main-nav">
      <ul>
        <li><a href="https://www.willowcreek.org/watch/watch-online">Watch</a></li>
        <li><a href="https://www.willowcreek.org/get-involved/">Visit</a></li>
        <li><a href="https://www.willowcreek.org/get-involved/groups/">Groups</a></li>
        <li><a href="https://www.willowcreek.org/get-involved/volunteer/">Volunteer</a></li>
        <li><a href="https://www.willowcreek.org/give/">Give</a></li>
      </ul>
    </nav>
    <div class="items">
      <div class="account-btn">
        {% if CurrentPerson %}
          <a href="/myaccount" class="d-none d-md-flex">
            <span class="hello">Hello, {{ CurrentPerson.NickName }}</span>
            <span class="account">ACCOUNT</span>
          </a>
          <a href="/myaccount" class="d-flex d-md-none">
            <i class="fa fa-user"></i>
          </a>
        {% else %}
          <a href="/login" class="d-none d-md-flex">
            <span class="hello">Hello, sign in</span>
            <span class="account">ACCOUNT</span>
          </a>
          <a href="/login" class="d-flex d-md-none">
            <i class="fa fa-user"></i>
          </a>
        {% endif %}
      </div>
      <button type="button" class="mega-menu-btn navbar-toggle collapsed d-block js-mega-menu-button" data-toggle="collapse" data-target="#mega-menu" aria-expanded="false" aria-controls="mega-menu">
        <div class="hamburger-menu">
          <div></div>
          <div></div>
          <div></div>
        </div>
      </button>



