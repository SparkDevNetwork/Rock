<div id="search-input" class="closed bg-gray-dark soft-sides soft-half-ends xs-soft-half-sides xs-hard-ends width-full push-bottom xs-push-half-bottom shadowed flush">
    <div class="position-relative input-search-wrapper display-inline-block width-auto stretchy">
        <input type="text" spellcheck="false" data-search-input class="stretchy input-search hard jumbo text-white sans-serif stronger form-control input-lg" placeholder="{% if CurrentPerson %}Hi {% endif %}{{ CurrentPerson.NickName | WithFallback:', what', 'What','append' }} can we help you find?" style="border: 0px; box-shadow: none;">
        <i data-search-input-clear class="input-clear position-absolute far fa-2x fa-backspace hidden"></i>
    </div>
</div>

<script src="~~/Scripts/stretchy.js" data-filter=".stretchy" async></script>