{% comment %} {% include '~~/Assets/Lava/Custom/SiteFooter/SiteFooter.lava' %} {% endcomment %}

{% assign logoId = CurrentPage.Layout.Site.SiteLogoBinaryFileId %}
{% assign primaryCampus = CurrentPerson.PrimaryCampusId | FromCache:'Campus' %}
{% unless primaryCampus and primaryCampus != empty %}
    {% assign primaryCampus = 1 | FromCache:'Campus' %}
{% endunless %}
{% assign streetOne = primaryCampus.Location.Street1 %}
{% assign streetTwo = primaryCampus.Location.Street2 %}
{% assign city = primaryCampus.Location.City %}
{% assign state = primaryCampus.Location.State %}
{% assign postalCode = primaryCampus.Location.PostalCode %}

<div class="primary-footer">
    <div class="container container-lg">
        <div class="row">
            <div class="col-md-4">
                {% comment %} brand {% endcomment %}
                <div class="primary-footer-brand">
                    {% if logoId and logoId != empty %}
                        <a href="/" class="primary-footer-brand-img text-center text-md-left">
                            <img src="/GetImage.ashx?Id={{ logoId }}">
                        </a>
