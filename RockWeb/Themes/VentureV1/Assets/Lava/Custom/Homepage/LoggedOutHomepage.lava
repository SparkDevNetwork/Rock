            }
        });

        $('#inputPostalCode').keypress(function (e) {
            var key = e.which;
            if (key == 13 &&  $('#inputPostalCode').val() != '') { // enter
                window.location.replace(`/campuses?location=${$('#inputPostalCode').val()}#locations`);
            }
        });
    });
</script>
