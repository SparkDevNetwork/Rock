        <a href="/series/{{ seriesSlug }}/{{ messageSlug }}" class="btn btn-primary btn-grow">Watch Now <i class="fa fa-play ml-2"></i></a>
        <a href="/media" class="btn btn-default btn-grow">See all Messages</a>
    </div>
{%- endcapture -%}

{%- capture latestMessageDetail -%}
