{% stylesheet id:'heroFeatureCardsv2' compile:'less' %}
    .hero-feature-cards {
        @media (min-width: 992px) {
            margin-top: -104px;
        }

        input[type="number"] {
            height: 40px;
        }
    }

    .homepage-swiper-gallery {
        background: linear-gradient(0deg, #F8F9FA 64px, #ffffff 64px);
    }

    .swiper-section {
        background: linear-gradient(180deg, #E9ECEF 50%, #F8F9FA 50.1%);

        &-card {
            display: flex;
            flex-direction: column;
            align-items: center;
            background-color: #fff;
            overflow: hidden;

            padding: 30px 45px;

            &-inner {
                width: 100%;
                margin: 0 auto;
                position: relative;
            }

            @media (min-width: 768px) {
                padding: 48px;
                // todo: .rounded-md();
                border-radius: 12px;
                // todo: .shadow-lg();
                box-shadow: 0px 8px 16px rgba(0, 0, 0, 0.08);
            }

            @media (min-width: 992px) {
                padding: 60px 100px;
            }
        }
    }
{% endstylesheet %}

