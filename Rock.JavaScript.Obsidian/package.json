{
  "private": true,
  "description": "Project for all Rock Obsidian UI functionality.",
  "scripts": {
    "build-fast": "node build/build-fast.js",
    "build": "npm run build-core && npm run build-framework && npm run copy-webforms-core && npm run copy-webforms-framework && echo '' > dist/.buildstamp && echo DONE",
    "build-framework": "tsc --build Framework && rollup --config Framework/rollup.config.js",
    "build-core": "webpack --no-color && lessc --js System/vendor.less dist/obsidian-vendor.css && cleancss --source-map dist/obsidian-vendor.css -o dist/obsidian-vendor.min.css",
    "watch": "echo \"Watch is currently not supported.\" && exit 1 && rollup --config Framework/rollup.config.js --watch",
    "copy-webforms-core": "copyfiles -u 1 \"dist/obsidian-core.*\" \"dist/obsidian-vendor.*\" \"../RockWeb/Obsidian/\"",
    "copy-webforms-framework": "copyfiles -u 2 \"dist/FrameworkBundled/**/*\" \"../RockWeb/Obsidian/\"",
    "clean": "rimraf dist",
    "test": "jest"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/dragula": "^3.7.1",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.0.3",
    "@types/luxon": "^2.0.3",
    "@types/node": "^14.14.7",
    "@types/systemjs": "^6.1.1",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "@vue/test-utils": "^2.0.0-rc.17",
    "clean-css-cli": "5.3.0",
    "copyfiles": "^2.4.1",
    "cssnano": "5.1.8",
    "eol": "^0.9.1",
    "eslint": "^7.32.0",
    "glob": "^7.2.0",
    "jest": "^27.4.3",
    "jest-editor-support": "^30.0.2",
    "less": "4.1.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "rollup-plugin-vue": "^6.0.0",
    "terser-webpack-plugin": "^5.1.4",
    "ts-jest": "^27.1.0",
    "ts-loader": "^9.1.2",
    "ts-node": "^10.4.0",
    "ttypescript": "^1.5.13",
    "typescript": "4.5.5",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "ant-design-vue": "3.2.1",
    "axios": "^0.21.4",
    "highlight.js": "11.5.1",
    "luxon": "^2.0.2",
    "mitt": "^2.1.0",
    "systemjs": " ^6.8.3",
    "vue": "3.2.35"
  }
}
