;  + MDX
;------------------------------------------------------------------------------
[MTMDX.NT.5.1]
CopyFiles=MTSTDUSB_CopyFiles,MTSTDUSBPP_CopyFiles
AddReg=MTMDX.AddReg.NT.5.1

[MTMDX.NT.5.1.Services]
