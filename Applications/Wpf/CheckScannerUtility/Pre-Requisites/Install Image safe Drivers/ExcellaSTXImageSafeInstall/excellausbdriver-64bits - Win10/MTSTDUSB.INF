HKR,,Port.RXQueueSize,0x00010001,0x00500000
HKR,,Port.RXBufferSize,0x00010001,0x0000F800
HKR,,Port.RXTransferSize,0x00010001,0x00002000
HKLM,%SERVICEROOT%
HKLM,%DEVICEROOT%


;------------------------------------------------------------------------------
