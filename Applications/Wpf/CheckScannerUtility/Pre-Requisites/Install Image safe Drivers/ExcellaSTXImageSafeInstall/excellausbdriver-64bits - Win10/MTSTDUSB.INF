;------------------------------------------------------------------------------
;  - MDXG2
;  + Windows 2000/XP Common Sections
;------------------------------------------------------------------------------

[MTSTDUSB_SVC.NT.5.1]
DisplayName    = %MTSTDUSB_DESC%
ServiceType=1
StartType=3
ErrorControl=1
ServiceBinary=%10%\system32\drivers\MTSTDUSB.sys
LoadOrderGroup = Extended Base

