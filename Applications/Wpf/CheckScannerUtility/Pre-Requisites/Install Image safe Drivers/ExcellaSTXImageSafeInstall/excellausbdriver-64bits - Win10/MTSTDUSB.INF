[HWAddRegMDX.NT.5.1]
HKR,,FriendlyName,,%FNAMEMDX%
HKR,,Port.RXQueueSize,0x00010001,0x00500000
HKR,,Port.RXBufferSize,0x00010001,0x00500000
HKR,,Port.RXTransferSize,0x00010001,0x00100000
HKLM,%SERVICEROOT%
HKLM,%DEVICEROOT%

