<Page
    x:Class="Rock.Apps.CheckScannerUtility.MicrImageHostPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:ax2="clr-namespace:AxMTMicrImage;assembly=AxMTMicrImage"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="MicrImageHostPage"
    d:DesignHeight="300"
    d:DesignWidth="300"
    mc:Ignorable="d">

    <Grid>
        <WindowsFormsHost x:Name="micrImageHost" Visibility="Collapsed">
            <ax2:AxMicrImage x:Name="micrImage" />
        </WindowsFormsHost>
    </Grid>
</Page>
